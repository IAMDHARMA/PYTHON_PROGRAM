   "source": [
    "8. Find Factorial of a Number\n",
    "\n",
    "Sample Input:\n",
    "\n",
    "n = 5\n",
    "\n",
    "Sample Output:\n",
    "\n",
    "Factorial: 120\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "76194585",
   "metadata": {},
   "outputs": [],
   "source": [
    "n = 5\n",
    "factorial = 1\n",
    "\n",
    "for i in range(1, n + 1):\n",
    "    factorial *= i\n",
    "\n",
    "print(\"Factorial:\", factorial)\n"
   ]
  }
