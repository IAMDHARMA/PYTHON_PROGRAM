    "def is_unique(s):\n",
    "    seen = set()\n",
    "    for char in s:\n",
    "        if char in seen:\n",
    "            return False\n",
    "        seen.add(char)\n",
    "    return True\n",
    "\n",
    "# Example usage\n",
    "print(is_unique(\"hello\"))   # False\n",
    "print(is_unique(\"world\"))   # True"
   ]
  }
