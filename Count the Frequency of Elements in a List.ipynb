    "def count_the_elements(lst):\n",
    "    d={}\n",
    "    for i in lst:\n",
    "        if i in d:\n",
    "            d[i]+=1\n",
    "        else:\n",
    "            d[i]=1\n",
    "    return d\n",
    "my_list = [1, 2, 2, 3, 4, 4, 4, 5]\n",
    "result=count_the_elements(my_list)\n",
    "print(result)\n"
   ]
