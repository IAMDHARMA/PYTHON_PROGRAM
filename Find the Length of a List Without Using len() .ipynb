   "source": [
    "def list_length(lst):\n",
    "    count = 0\n",
    "    for _ in lst:\n",
    "        count += 1\n",
    "    return count\n",
    "\n",
    "# Example usage\n",
    "my_list = [10, 20, 30, 40]\n",
    "print(\"Length:\", list_length(my_list))\n"
   ]
