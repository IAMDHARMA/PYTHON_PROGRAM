   "source": [
    "def is_palindrome(lst):\n",
    "    return lst == lst[::-1]\n",
    "\n",
    "# Example usage\n",
    "print(is_palindrome([1, 2, 3, 2, 1]))  # True\n",
    "print(is_palindrome([1, 2, 3, 4]))     # False"
   ]
