    "def search_contact(contact_book, name):\n",
    "    return f\"Contact: {contact_book.get(name, 'Not found')}\"\n",
    "\n",
    "# Example\n",
    "contacts = {\"Ravi\": \"9876543210\", \"Anu\": \"9911223344\"}\n",
    "print(search_contact(contacts, \"Anu\"))  # Output: Contact: 9911223344\n"
   ]
