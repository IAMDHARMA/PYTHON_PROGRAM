   "source": [
    "#Find Common Elements Between Two Lists\n",
    "l=[i for i in range(10) ]\n",
    "l2=[i for i in range(10) if i%2==0]\n",
    "l3=[]\n",
    "print(l,l2)\n",
    "for i in l:\n",
    "    for j in l2:\n",
    "        if i==j:\n",
    "            l3.append(i)\n",
    "print(l3)\n"
   ]
  }
