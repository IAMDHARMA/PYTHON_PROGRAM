   "source": [
    "def flatten_list(nested_list):\n",
    "    flat = []\n",
    "    for sublist in nested_list:\n",
    "        for item in sublist:\n",
    "            flat.append(item)\n",
    "    return flat\n",
    "\n",
    "# Example\n",
    "print(flatten_list([[1, 2], [3, 4], [5]]))  # Output: [1, 2, 3, 4, 5]\n"
   ]
  }
